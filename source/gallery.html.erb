<div class="photoalbum">
  <div class="row">
    <% data.photoalbum.each do |photo| %>
      <div class="col-xs-6 col-md-4 col-lg-3">
        <div class="photo">
          <%= image_tag photo.name %>
          <div class="description">
            <%= photo.description %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
